{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**"
          ]
        }
      ],
      "dest": "api/Unity",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/Unity/**.yml",
          "api/Unity/index.md"
        ]
      },
      {
        "files": ["**/*.yml"],
        "src": "yml",
        "dest": "api/Alexa"
      },
      {
        "files": [
          "articles/tutorials/**.md",
          "articles/tutorials/**/toc.yml",
          "articles/getting_started/**.md",
          "articles/getting_started/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "custom_template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appLogoPath": "images/largeLogo.png",
      "_appFooter": "Copyright Â© 2018 Austin Wilson"
    }
  }
}